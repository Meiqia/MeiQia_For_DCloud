{
	"name": "MeiQiaChatPlugin",
	"id": "MeiQiaChatPlugin",
	"version": "1.0.0",
	"description": "美洽官方Hbuilder插件",
	"_dp_type": "nativeplugin",
	"_dp_nativeplugin": {
		"ios": {
			"plugins": [{
				"type": "module",
				"name": "MeiQiaChatPlugin",
				"class": "MeiQiaChatModule"
			}],
			"hooksClass": "MeiQiaChatPluginProxy",
			"integrateType": "framework",
			"deploymentTarget": "9.0",
			"frameworks": [
				"MeiQiaSDK.framework"
			],
			"embedFrameworks": [
				"MeiQiaSDK.framework"
			],
			"resources": [
				"MQChatViewAsset.bundle"
			],
			"parameters": {
				"AppKey": {
					"des": "平台的appkey",
					"key": "MeiQiaChatPlugin:appkey"
				}
			}
		},
		"android": {
			"plugins": [{
				"type": "module",
				"name": "MeiQiaChatPlugin",
				"class": "com.meiqia.uniplugin_meiqia.MeiQiaChatPlugin"
			}],
			"parameters": {
				"appkey": {
					"des": "美洽平台的 appkey",
					"meiqia_appkey": "MeiQiaChatPlugin:appkey"
				}
			},
			"dependencies": [{
				"source": "implementation 'com.meiqia:meiqiasdk:3.7.8'"
			},
				{
					"source": "implementation 'com.squareup.okhttp3:okhttp:3.5.0'"
				},
				{
					"source": "'com.github.bumptech.glide:glide:4.9.0'"
				},
				{
					"source": "'com.github.bumptech.glide:compiler:4.9.0'"
				},
			],
			"integrateType": "aar",
			"minSdkVersion": "19",
			"permissions": [
				"android.permission.INTERNET",
				"android.permission.WRITE_EXTERNAL_STORAGE",
				"android.permission.ACCESS_NETWORK_STATE",
				"android.permission.CAMERA",
				"android.permission.RECORD_AUDIO"
			]
		}
	}
}
