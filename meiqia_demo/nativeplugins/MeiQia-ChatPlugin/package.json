{
    "name": "MeiQiaChatPlugin",
    "id": "MeiQia-ChatPlugin",
    "version": "1.2.4",
    "description": "美洽官方 Hbuilder 插件",
    "_dp_type": "nativeplugin",
    "_dp_nativeplugin": {
        "ios": {
            "plugins": [{
                "type": "module",
                "name": "MeiQia-ChatPlugin",
                "class": "MeiQiaChatModule"
            }],
            "hooksClass": "MeiQiaChatPluginProxy",
            "integrateType": "framework",
            "deploymentTarget": "11.0",
            "validArchitectures": [    
                    "arm64"
            ],
            "frameworks": [
                "MeiQiaSDK.framework"
            ],
            "embedFrameworks": [
                "MeiQiaSDK.framework"
            ],
            "parameters": {
                "MEIQIA_APPKEY_IOS": {
                    "des": "美洽平台的Appkey_iOS",
                    "key": "MeiQiaChatPlugin:appkey"
                }
            }
        },
        "android": {
            "plugins": [{
                "type": "module",
                "name": "MeiQia-ChatPlugin",
                "class": "com.meiqia.uniplugin_meiqia.MeiQiaChatPlugin"
            }],
            "parameters": {
                "MEIQIA_APPKEY_ANDROID": {
                    "des": "美洽平台的Appkey_Android",
                    "key": "meiqia_appkey"
                }
            },
            "dependencies": [
                "com.meiqia:androidx:4.1.4"
            ],
            "abis": [
                "armeabi-v7a",
                "arm64-v8a",
                "x86"
            ],
            "integrateType": "aar",
            "minSdkVersion": 21,
            "useAndroidX": true,
            "permissions": [
                "android.permission.INTERNET",
                "android.permission.WRITE_EXTERNAL_STORAGE",
                "android.permission.ACCESS_NETWORK_STATE",
                "android.permission.CAMERA",
                "android.permission.RECORD_AUDIO"
            ]
        }
    }
}
